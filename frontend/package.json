{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:frontend": "yarn dev",
    "dev:user": "cd ../backend/user-service && yarn dev",
    "dev:assignment": "cd ../backend/assignment-service && yarn pure-dev",
    "dev:grading": "cd ../backend/grading-service && yarn pure-dev",
    "dev-all": "concurrently --kill-others \"yarn dev:frontend\" \"yarn dev:grading\" \"yarn dev:assignment\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && prettier --check --ignore-path .gitignore .",
    "lint:autofix": "next lint --fix && prettier --write --ignore-path .gitignore .",
    "test": "jest",
    "test:update": "jest -u",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "@nextui-org/react": "^2.2.9",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/react-query": "^5.24.1",
    "axios": "^1.6.7",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.5.1",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.22",
    "html-react-parser": "^5.1.8",
    "lucide-react": "^0.363.0",
    "js-cookie": "^3.0.5",
    "monaco-editor": "^0.47.0",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.0.1",
    "react-quill": "^2.0.0",
    "react-toastify": "^10.0.5",
    "tailwind": "^4.0.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "typescript-eslint": "^7.2.0"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  }
}
