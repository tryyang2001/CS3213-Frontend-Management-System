  "main": "index.js",
  "license": "MIT",
  "prisma": {
    "schema": "src/models/prisma/schema.prisma"
  },
  "scripts": {
    "dev": "prisma generate && nodemon src/app.ts",
    "pure-dev": "nodemon src/app.ts",
    "build": "prisma generate && tsc -p .",
    "start": "node dist/app.js",
